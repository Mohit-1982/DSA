GFG

class Solution {
    public Pair<Node, Node> splitList(Node head) {
        Node slow = head;
        Node fast = head;
        while(fast.next != head && fast.next.next != head){
            slow = slow.next;
            fast = fast.next.next;
        }
        Node a = slow.next;
        slow.next = head;
        Node temp = a;
        while(temp.next != head){
            temp = temp.next;
        }
        temp.next = a;
        return new Pair<>(head,a);
    }
}
